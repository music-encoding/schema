            &lt;/beam&gt;
                &lt;note dur="4" oct="4" pname="e" xml:id="analysis.n_sc_73_1"/&gt;
                &lt;note dur="4" oct="4" pname="e" xml:id="analysis.n_sc_75_1"/&gt;
            &lt;beam&gt;
                &lt;note dur="8" oct="4" pname="e" xml:id="analysis.n_sc_76_1"/&gt;
