  </classSpec><classSpec rend="add" predeclare="true" ident="model.staffDefPart" module="MEI.shared" type="model">
    <desc xml:lang="en">Groups elements that may appear in the declaration of staff features.</desc>
  </classSpec><classSpec rend="add" predeclare="true" ident="model.staffGrpLike" module="MEI.shared" type="model">
