      <zeroOrMore xmlns="http://relaxng.org/ns/structure/1.0"><ref name="seriesStmt"/></zeroOrMore>
    </content>
  </macroSpec><classSpec rend="add" ident="att.bifoliumSurfaces" module="MEI.header" type="atts">
    <desc xml:lang="en">Attributes that link a bifolium element with a <gi scheme="MEI">surface</gi>
