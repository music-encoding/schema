          </valItem>
          <valItem ident="markup">
            <desc xml:lang="en">Corrections and normalizations represented using markup.</desc>
          </valItem>
        </valList>
      </attDef>
    </attList>
  </classSpec><classSpec rend="add" predeclare="true" ident="model.bifoliumLike" type="model" module="MEI.header">
