        </datatype>
      </attDef>
    </attList>
  </classSpec><classSpec rend="add" ident="att.rdg.log" module="MEI.critapp" type="atts">
    <desc xml:lang="en">Logical domain attributes.</desc>
  </classSpec><classSpec rend="add" predeclare="true" ident="model.appLike" module="MEI.critapp" type="model">
    <desc xml:lang="en">Groups elements that contain a critical apparatus entry.</desc>
  </classSpec><classSpec rend="add" predeclare="true" ident="model.rdgPart" module="MEI.critapp" type="model">
    <desc xml:lang="en">Groups elements that may appear as part of a textual or musical variant.</desc>
  </classSpec><classSpec rend="add" predeclare="true" ident="model.rdgPart.music" module="MEI.critapp" type="model">
